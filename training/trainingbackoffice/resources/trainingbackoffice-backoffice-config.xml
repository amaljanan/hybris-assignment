	<context merge-by="module" component="editorareaactions" type="Consignment">
		<y:actions xmlns:y="http://www.hybris.com/cockpit/config/hybris">
			<y:group qualifier="common">
				<y:label>actiongroup.common</y:label>				
				<y:action action-id="de.hybris.training.backoffice.actions.confirmpickupaction" property="currentObject"/>				
			</y:group>
		</y:actions>
    </context>

	<!-- Notification Area-->
	<context component="notification-area">
	    <na:notification-area>	        
	        <na:notifications eventType="trainingbackoffice.confirmpickup.event" level="SUCCESS">
	            <na:message>hmc.action.confirmpickup.success</na:message>
	        </na:notifications>
	   </na:notification-area>
	</context>

	<!-- Tree node-->
	<context merge-by="module"  type="trainingbackoffice" component="explorer-tree">
		<explorer-tree:explorer-tree>
			<explorer-tree:navigation-node id="Rent_Vehicle" merge-mode="append">
				<explorer-tree:type-node  code="Vehicle" id="Vehicle" />
				<explorer-tree:type-node  code="Driver" id="Driver" />
				<explorer-tree:type-node  code="Dealer" id="Dealer" />
			</explorer-tree:navigation-node>
		</explorer-tree:explorer-tree>
	</context>

<!--  Tree node for vehicle accessories	-->
	<context merge-by="module"  type="trainingbackoffice" component="explorer-tree">
		<explorer-tree:explorer-tree>
			<explorer-tree:navigation-node id="Vehicle_Accessories" merge-mode="append">
				<explorer-tree:type-node  code="VehicleAccessories" id="VehicleAccessories" />
				<explorer-tree:type-node  code="AccessoriesVehicleVariantProduct" id="AccessoriesVehicleVariantProduct" />
				<explorer-tree:type-node  code="AccessoriesColorVariantProduct" id="AccessoriesColorVariantProduct" />
			</explorer-tree:navigation-node>
		</explorer-tree:explorer-tree>
	</context>


<!--  Vehicle accessories   -->

    <context merge-by="type" type="VehicleAccessories" component="editor-area">
        <editorArea:editorArea>
            <editorArea:essentials>
                <editorArea:essentialSection  name="hmc.section.accessories.attributes">
                    <editorArea:attribute qualifier="catalogVersion" />
                    <editorArea:attribute qualifier="code" />
                    <editorArea:attribute qualifier="approvalStatus" />
                    <editorArea:attribute qualifier="name" />
                </editorArea:essentialSection >
            </editorArea:essentials>
			<editorArea:tab name="hmc.tab.accessories.essential"
							position="1">
			</editorArea:tab>
        </editorArea:editorArea>
    </context>



    <context type="VehicleAccessories" component="editorAreaDynamicForms" merge-by="module">
        <df:dynamicForms xmlns:df="http://www.hybris.com/cockpitng/component/dynamicForms">
            <df:attribute id="accessories.dynamic" qualifier="description" triggeredOn="name" computedValue="'Perfect ' + name[new java.util.Locale('en')] + ' for your car '"/>
        </df:dynamicForms>
    </context>

    <context type="VehicleAccessories" component="base" merge-by="type" parent="GenericItem">
        <y:base>
            <y:labels>
                <y:label>name+ '-' +catalogVersion</y:label>
            </y:labels>
        </y:base>
    </context>

    <context merge-by="type"  type="VehicleAccessories" component="simple-search">
        <simple-search:simple-search>
            <simple-search:field name="name" />
            <simple-search:field name="code" />
            <simple-search:field name="material" />
        </simple-search:simple-search>
    </context>

	<context merge-by="type" parent="GenericItem" type="VehicleAccessories" component="advanced-search">
		<advanced-search:advanced-search>
			<advanced-search:field-list>
				<advanced-search:field name="material" selected="true" operator="equals" />
			</advanced-search:field-list>
		</advanced-search:advanced-search>
	</context>

	<context merge-by="type" parent="GenericItem" type="VehicleAccessories" component="listview">
		<list-view:list-view >
			<list-view:column qualifier="code" />
			<list-view:column qualifier="name" />
			<list-view:column qualifier="catalogVersion" />
			<list-view:column qualifier="material" />
		</list-view:list-view>
	</context>

	<context  type="VehicleAccessories" Parent="Product" component="create-wizard">
		<wz:flow  id="VehicleAccessoriesWizard" title="create.accessories.title">
			<wz:prepare id="VehicleAccessoriesPrepare">
				<wz:initialize property="newAccessory" type="ctx.TYPE_CODE"/>
			</wz:prepare>
			<wz:step id="step1" label="create.accessories.essential.label" sublabel="create.accessories.essential.sublabel">
				<wz:info id="step1.intro" label="create.accessories.essential.intro" position="top"/>
				<wz:content id="step1.content">
					<wz:property-list root="newAccessory">
						<wz:property qualifier="name" type="java.lang.String"/>
						<wz:property qualifier="code"  />
						<wz:property qualifier="approvalStatus" />
						<wz:property qualifier="catalogVersion"  />
					</wz:property-list>
				</wz:content>
				<wz:navigation id="step1.navigation">
					<wz:cancel/>
					<wz:next visible = "(newAccessory.code != null) and (newAccessory.catalogVersion != null)" />
					<wz:done visible= "(newAccessory.code != null) and (newAccessory.catalogVersion != null) ">
						<wz:save property="newAccessory"/>
					</wz:done>
				</wz:navigation>
			</wz:step>
			<wz:step id="step2" label="create.accessories.basic.label" sublabel="create.dealer.basic.sublabel">
				<wz:info id="step1.intro" label="create.accessories.basic.intro" position="top"/>
				<wz:content id="step2.content">
					<wz:property-list root="newAccessory">
						<wz:property qualifier="material" type="java.lang.String"/>
					</wz:property-list>
				</wz:content>
				<wz:navigation id="step2.navigation">
					<wz:cancel/>
					<wz:next />
					<wz:done >
						<wz:save property="newAccessory"/>
					</wz:done>
				</wz:navigation>
			</wz:step>
		</wz:flow>
	</context>

