			<wz:prepare id="AccessoriesVariantPrepare">
				<wz:initialize property="newVariant" type="ctx.TYPE_CODE"/>
			</wz:prepare>
			<wz:step id="step1" label="create.variant.essential.label" sublabel="create.variant.essential.sublabel">
				<wz:info id="step1.intro" label="create.variant.essential.intro" position="top"/>
				<wz:content id="step1.content">
					<wz:property-list root="newVariant">
