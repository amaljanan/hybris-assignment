				</wz:navigation>
			</wz:step>
			<wz:step id="step2" label="create.variant.info.label" sublabel="create.variant.info.sublabel">
				<wz:info id="step1.intro" label="create.variant.info.intro" position="top"/>
				<wz:content id="step2.content">
					<wz:property-list root="newVariant">
						<wz:property qualifier="color" type="java.lang.String"/>
                        <wz:property qualifier="baseProduct"/>
					</wz:property-list>
				</wz:content>
				<wz:navigation id="step2.navigation">
					<wz:cancel/>
					<wz:next />
                    <wz:done visible= "(newVariant.baseProduct != null)">
                        <wz:save property="newVariant"/>
                    </wz:done>
				</wz:navigation>
			</wz:step>
		</wz:flow>
	</context>

