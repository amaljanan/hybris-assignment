<beans xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="beans.xsd">

	<bean class="de.hybris.platform.commercefacades.product.data.ProductData">
		<property name="genders" type="java.util.List&lt;de.hybris.training.facades.product.data.GenderData>"/>
		<property name="registrationNumber" type="String"/>
		<property name="dealerName" type="String" />
		<property name="driverName" type="String" />
		<property name="supportedVehicle" type="String" />
		<property name="color" type="String" />
	</bean>

	<bean class="de.hybris.training.facades.product.data.GenderData">
		<property name="code" type="String"/>
		<property name="name" type="String"/>
	</bean>
	<bean class="de.hybris.training.facades.product.data.VehicleData">
		<property name="registrationNumber" type="String"/>
		<property name="code" type="String"/>
	</bean>

	<bean class="de.hybris.training.facades.product.data.DealerData" >
		<property name="dealerId" type="int" />
		<property name="dealerName" type="String" />
	</bean>

	<bean class="de.hybris.training.facades.product.data.DriverData" >
		<property name="driverId" type="int" />
		<property name="driverName" type="String" />
		<property name="experience" type="int" />
	</bean>

	<bean class="de.hybris.training.facades.product.data.VehicleAccessoriesData" >
		<property name="code" type="String" />
		<property name="name" type="String" />
		<property name="material" type="String" />
	</bean>

	<bean class="de.hybris.training.facades.product.data.AccessoriesVehicleVariantData" >
		<property name="code" type="String" />
		<property name="name" type="String" />
		<property name="supportedVehicle" type="String" />
	</bean>

	
</beans>
